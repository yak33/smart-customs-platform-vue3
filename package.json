{
  "name": "smart-customs-platform",
  "type": "module",
  "version": "0.0.2",
  "description": "智慧关务平台，专注于提供智能化的海关业务管理解决方案",
  "author": {
    "name": "ZHANGCHAO",
    "email": "vuanne1@gmail.com",
    "url": "https://github.com/yak33/smart-customs-platform-vue3"
  },
  "license": "MIT",
  "homepage": "https://github.com/yak33/smart-customs-platform-vue3",
  "repository": {
    "url": "https://github.com/yak33/smart-customs-platform-vue3.git"
  },
  "bugs": {
    "url": "https://github.com/yak33/smart-customs-platform-vue3/issues"
  },
  "keywords": [
    "smart-customs-platform-server",
    "smart-customs-platform-vue3",
    "Vue3 admin",
    "vue-admin-template",
    "Vite7",
    "TypeScript",
    "naive-ui",
    "naive-ui-admin",
    "ant-design-vue v4",
    "UnoCSS"
  ],
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.5.0"
  },
  "scripts": {
    "build": "vite build --mode prod",
    "build:dev": "vite build --mode dev",
    "build:test": "vite build --mode test",
    "clean": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.vite && rimraf node_modules/.cache",
    "cleanup": "sa cleanup",
    "commit": "sa git-commit",
    "commit:zh": "sa git-commit -l=zh-cn",
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:test": "vite --mode test",
    "gen-route": "sa gen-route",
    "install:force": "pnpm install --force",
    "lint": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview",
    "reinstall": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "5.0.0",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/tinymce": "workspace:*",
    "@sa/utils": "workspace:*",
    "@types/streamsaver": "^2.0.5",
    "@umoteam/editor": "^8.1.0",
    "@vueuse/core": "13.9.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.18",
    "defu": "6.1.4",
    "echarts": "6.0.0",
    "highlight.js": "^11.11.1",
    "jsencrypt": "^3.3.2",
    "json5": "2.2.3",
    "monaco-editor": "^0.52.2",
    "naive-ui": "2.43.1",
    "nprogress": "0.2.0",
    "pinia": "3.0.3",
    "streamsaver": "^2.0.6",
    "tailwind-merge": "3.3.1",
    "vue": "3.5.21",
    "vue-advanced-cropper": "^2.8.9",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "11.1.12",
    "vue-router": "4.5.1",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@elegant-router/vue": "0.3.8",
    "@iconify/json": "2.2.385",
    "@sa/scripts": "workspace:*",
    "@sa/uno-preset": "workspace:*",
    "@soybeanjs/eslint-config": "1.7.1",
    "@types/node": "24.5.1",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "66.5.1",
    "@unocss/preset-icons": "66.5.1",
    "@unocss/preset-uno": "66.5.1",
    "@unocss/transformer-directives": "66.5.1",
    "@unocss/transformer-variant-group": "66.5.1",
    "@unocss/vite": "66.5.1",
    "@vitejs/plugin-vue": "6.0.1",
    "@vitejs/plugin-vue-jsx": "5.1.1",
    "consola": "3.4.2",
    "eslint": "9.35.0",
    "eslint-plugin-vue": "10.4.0",
    "kolorist": "1.8.0",
    "rimraf": "6.0.1",
    "sass": "1.92.1",
    "simple-git-hooks": "2.13.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "unplugin-icons": "22.3.0",
    "unplugin-vue-components": "29.0.0",
    "vite": "7.1.5",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-static-copy": "^3.1.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "8.0.2",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.0.7"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm sa git-commit-verify",
    "pre-commit": "pnpm typecheck && pnpm lint && git diff --exit-code"
  },
  "website": "https://github.com/yak33/smart-customs-platform-vue3"
}
